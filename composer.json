{
    "name": "hanaboso/app-store",
    "description": "App store for PipesFramework",
    "version": "1.5.0",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^7.4",
        "hanaboso/pipes-php-sdk": "^1.5"
    },
    "require-dev": {
        "hanaboso/php-check-utils": "^1.1",
        "hanaboso/rest-bundle": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Hanaboso\\HbPFAppStore\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HbPFAppStoreTests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../pipes-php-sdk",
            "options": {
                "symlink": true
            }
        }
    ]
}
